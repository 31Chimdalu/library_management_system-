<!DOCTYPE html>
<html>
<head>
  <title>Library Books</title>
  <link rel="stylesheet" href="/css/styles.css">
</head>
<body>
  <h1>Available Books</h1>
  <ul>
    <% books.forEach(book => { %>
      <li><%= book.title %> by <%= book.author %> (ISBN: <%= book.ISBN %>)</li>
    <% }) %>
  </ul>
  <a href="/">Back to Home</a>
</body>
</html>
